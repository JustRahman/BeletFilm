<Window x:Class="Belet.Views.ChoosePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        
    
            
             xmlns:ChoosePageViewModel="clr-namespace:Belet.ViewModels"
             xmlns:dxmvvm1="clr-namespace:DevExpress.Mvvm.UI;assembly=DevExpress.Xpf.Core.v21.1"
             xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:dxwuin="http://schemas.devexpress.com/winfx/2008/xaml/windowsui/navigation"
             xmlns:ViewModels="clr-namespace:Belet.ViewModels" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
            WindowStartupLocation="CenterScreen" MouseLeftButtonDown="WindowMouseLeftButtonDown"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
        xmlns:local="clr-namespace:Belet.Views" 
        x:Name="Window"
        xmlns:converters="clr-namespace:Belet.Helper"
          TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        Background="{x:Null}"  
           xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
         WindowStyle="None"
        AllowsTransparency="True"
        mc:Ignorable="d"  MinWidth="600" 
        Title="Window2" Height="800"
             Width="1300">


   
    <Window.DataContext>
        <ViewModels:ChoosePageViewModel/> 
    </Window.DataContext>



    <x:Code>
        <![CDATA[            
        private void WindowMouseLeftButtonDown(object sender, MouseButtonEventArgs e)
        {
            DragMove();
        }
    ]]>
    </x:Code>


    <Window.Resources>

        <converters:dk_MultiBindingConverter x:Key="MultiBindingConverter" />




        <Image x:Key="Agent"
               Source="{Binding choosePage.FilmAgent}" Width="500" />
        <Image x:Key="Beletagts"
               Source="{Binding choosePage.ASTUPage}" />
        <Image x:Key="BeletArakesme"
               Source="{Binding choosePage.ArakesmePage}" />
        <Image x:Key="beletTuner"
               Source="{Binding choosePage.TunerPage}" />
        <Image x:Key="BeletTV"
               Source="{Binding choosePage.TVbox}"
               Margin="20" />
        <Image x:Key="MashaandBear"
               Source="{Binding choosePage.MashaBear}"/>
        <Image x:Key="Turkmenteleckom"
               Source="{Binding choosePage.TelekomPage}" />
        
    </Window.Resources>

  






    <Grid Background="{DynamicResource MaterialDesignPaper}">
        <Grid.RowDefinitions>
            <RowDefinition Height="auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        
        


        <materialDesign:PopupBox Grid.Row="0" HorizontalAlignment="Right" Margin="0 10 20 10" PlacementMode="BottomAndAlignRightEdges" StaysOpen="False" Height="25">

            <StackPanel> 
                <StackPanel Margin="16 10 0 6" Orientation="Horizontal" HorizontalAlignment="Center">
                    <TextBlock VerticalAlignment="Center" Text="Dark Mode"/>
                    <ToggleButton  Cursor="Hand" ToolTip="Enable Dark Mode" Margin="12 0 8 0" x:Name="themeToggle" IsChecked="{Binding IsDarkTheme}" Command="{Binding toggleTheme}"/>
                </StackPanel>
                <Button Command="{Binding goldawmerkezi}" ToolTip="Having Trouble Loggin In?" Margin="0 8 0 0" Content="Help me"/>
                <Button   ToolTip="Close Application" Content="Exit Application" Command="{Binding close_application}"/>
                <Button   ToolTip="Minimize" Content="Minimize" Command="{Binding cmdLogin}"/>
                <Button   ToolTip="Close Application" Content="{Binding  choosePage.IsBigOrNormal}" Command="{Binding sizechangebtn}"/>
                <Button   ToolTip="Log out" x:Name="LogOutBtn" Content="Log out" Command="{Binding LogOutBtn}"/>
                <Button ToolTip="Present" Content="Present" Command="{Binding presentBtn}"/>
            </StackPanel>
        </materialDesign:PopupBox>
        
        
        



        <ScrollViewer Grid.Row="1"
                      VerticalScrollBarVisibility="Hidden" Height="755" Margin="0,0.2,0,0" VerticalAlignment="Top">



            <StackPanel >

                <dxmvvm:Interaction.Behaviors>
                    <dxmvvm:EventToCommand EventName="Loaded"   Command="{Binding InitializeCommand}">
                        <dxmvvm:EventToCommand.CommandParameter>
                            <MultiBinding Converter="{StaticResource MultiBindingConverter}">
                                <Binding ElementName="Window"/>
                                <Binding ElementName="LogOutBtn"/>
                            </MultiBinding>
                        </dxmvvm:EventToCommand.CommandParameter>
                    </dxmvvm:EventToCommand>

                    <dxmvvm:EventToCommand EventName="SizeChanged"   Command="{Binding size}">
                        <dxmvvm:EventToCommand.CommandParameter>
                            <MultiBinding Converter="{StaticResource MultiBindingConverter}">
                                <Binding ElementName="Window"/>
                            </MultiBinding>
                        </dxmvvm:EventToCommand.CommandParameter>
                    </dxmvvm:EventToCommand>

                </dxmvvm:Interaction.Behaviors>



                <StackPanel >

                    <Grid Margin="0 20 0 0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.7*"/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>



                        <WrapPanel Grid.Column="0" Margin="100 0 0 0">
                            <Image HorizontalAlignment="Left" Source="C:\Data\source\WPF\BeletFilm\Belet\bin\Debug\Images\BeletLogo.png" Width="80"/>

                        </WrapPanel>
                        <StackPanel  Width="auto" Grid.Column="1">
                            <WrapPanel Margin="0 0 100 0" HorizontalAlignment="Right" VerticalAlignment="Center" Orientation="Horizontal">
                                <Button   Height="auto"
                            Command="{Binding VideoFrame}" BorderThickness="0"   Style="{StaticResource MaterialDesignFlatButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" materialDesign:ButtonAssist.CornerRadius="10"  Background="Transparent" Width="auto" FontSize="13"  Content="Video"/>
                                <Button Height="auto"
                            BorderThickness="0"   
                            Background="Transparent"
                            Command="{Binding FilmFrame}"
                          Style="{StaticResource MaterialDesignFlatButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0" materialDesign:ButtonAssist.CornerRadius="10"
                            Width="auto"
                            FontSize="13"
                            Margin="0 0 10 0" Content="Film"/>
                                <ComboBox BorderThickness="0" 
                              Background="Transparent" FontSize="13" VerticalAlignment="Center" Width="auto" Margin="0 0 10 0">
                                    <ComboBoxItem  FontSize="13"  IsSelected="True" Content="TM"/>
                                </ComboBox>
                                <materialDesign:PackIcon Width='30' VerticalAlignment="Center" Height="30" Kind="AccountCircle" />

                                <ComboBox BorderThickness="0" Margin="0 0 20 0"  VerticalAlignment="Center"   Width="auto">
                                    <ComboBoxItem >
                                        <StackPanel>
                                            <TextBlock  Text="{Binding choosePage.PhonePersonNumber}"/>
                                        </StackPanel>

                                    </ComboBoxItem>
                                </ComboBox>
                            </WrapPanel>
                        </StackPanel>
                    </Grid>

                    <StackPanel Margin="0 15 0 0"
                                Height="130"
                                VerticalAlignment="Top"
                                HorizontalAlignment="Center">
                        <TextBlock HorizontalAlignment="Center"
                                   FontSize="40"
                                   FontWeight="DemiBold"
                                   Text="'Belet' her kim üçin" />
                        <TextBlock Margin="0 10 0 0"
                                   FontSize="20"
                                   Foreground="Gray"
                                   Text="Islendik ýerden we islendik enjamdan gyzykly mazmun" />
                    </StackPanel>



                    <Border VerticalAlignment="Bottom" Height="250" Width="250"   Margin="1350 -200 0 0" CornerRadius="100">
                        <Ellipse>
                            <Ellipse.Fill>
                                <ImageBrush  Opacity="0.6"  >
                                    <ImageBrush.ImageSource>
                                        <DrawingImage  PresentationOptions:Freeze="True">
                                            <DrawingImage.Drawing>
                                                <DrawingGroup>
                                                    <DrawingGroup>
                                                        <DrawingGroup.Transform>
                                                            <TranslateTransform X="14.6999998092651" Y="22.6000003814697"/>
                                                        </DrawingGroup.Transform>
                                                        <DrawingGroup>
                                                            <DrawingGroup.ClipGeometry>
                                                                <RectangleGeometry Rect="0,0,500,500"/>
                                                            </DrawingGroup.ClipGeometry>
                                                            <GeometryDrawing Pen="{StaticResource Pen10}" Geometry="F1M11.3,9.1L486.6,484 M-6.5,34.8L468.8,509.6 M498.2,319.4L181.5,2.7 M526.8,299.6L210.1,-17.1 M355.6,1L494.7,139.7 M275.9,3.1L492.6,219.4 M294.6,-17.6L511.3,198.7 M7.4,181.1L313.3,487 M-6.4,216.6L299.5,522.5 M3.1,86.3L416.8,499.9 M-12.7,116.8L401,530.5 M93.6,0.1L498.6,405.1 M115.3,-20.6L520.3,384.4 M81.8,33.6L486.8,438.7 M1.4,350.8L144.4,494.3 M3.5,275.4L219.8,492.2 M-11.3,300.1L205,516.8"/>
                                                        </DrawingGroup>
                                                    </DrawingGroup>
                                                </DrawingGroup>
                                            </DrawingImage.Drawing>
                                        </DrawingImage>
                                    </ImageBrush.ImageSource>
                                </ImageBrush>
                            </Ellipse.Fill>
                        </Ellipse>
                    </Border>

                    <WrapPanel  HorizontalAlignment="Center">
                        <Button  BorderThickness="0" Command="{Binding MashaBearbutton}" Margin="0 0 50 20"    HorizontalAlignment="Left" Background="{Binding choosePage.FirstBox}" Width="400" Height="400">
                            <Button.Content>
                                <StackPanel Width="490" Height="400" Margin="-15">
                                    <Button Margin="-80 20 0 0"
                                            BorderThickness="0" 
                                            Command="{Binding MashaBearbutton}"
                                            Background="Transparent"
                                            Content="{StaticResource MashaandBear}" 
                                            Height="260"/>
                                    <StackPanel Margin="0 30 0 0">
                                        <Button Command="{Binding MashaBearbutton}"  FontSize="20" HorizontalAlignment="Left" FontWeight="Medium" BorderThickness="0" Background="Transparent" TextElement.Foreground="{DynamicResource MaterialDesignBody}" Content="Belet Video" Height="auto"/>

                                        <Button  FontSize="17" HorizontalAlignment="Left" FontWeight="Medium"    BorderThickness="0" Background="Transparent" Content="Geç >" Style="{StaticResource MaterialDesignFlatButton}" Foreground="#377DFF" Command="{Binding MashaBearbutton}"    materialDesign:ShadowAssist.ShadowDepth="Depth0"   Width="auto" materialDesign:ButtonAssist.CornerRadius="10"  />

                                    </StackPanel>
                                </StackPanel>
                            </Button.Content>
                            <Button.Triggers>
                                <EventTrigger RoutedEvent="Button.MouseEnter" >
                                    <BeginStoryboard>
                                        <Storyboard >
                                            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="Margin" BeginTime="00:00:00">
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.0"  Value="0, 0,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.1"  Value="0, -1,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.12" Value="0, -2,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.15" Value="0, -3,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.17" Value="0, -4,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.2"  Value="0, -5,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.23" Value="0, -6,50,20" />

                                            </ThicknessAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </EventTrigger>
                                <EventTrigger RoutedEvent="Button.MouseLeave" >
                                    <BeginStoryboard>
                                        <Storyboard >
                                            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="Margin" BeginTime="00:00:00">

                                                <SplineThicknessKeyFrame KeyTime="0:0:0.0" Value="0, -6,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.1" Value="0, -5,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.12" Value="0, -4,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.15"  Value="0, -3,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.17" Value="0, -2,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.2"  Value="0, -1,50,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.23" Value="0, 0,50,20" />
                                            </ThicknessAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </EventTrigger>
                            </Button.Triggers>
                        </Button>

                        <Button BorderThickness="0" Margin="0 0 0 20"   Command="{Binding FilmFrame}"  HorizontalAlignment="Right" Background="{Binding choosePage.SecondBox}" Width="400" Height="400">
                            <Button.Content>
                                <StackPanel Width="490" Height="400" Margin="-15">
                                    <Button Margin="-80 20 0 0"
                                            BorderThickness="0"
                                            Command="{Binding FilmFrame}"
                                            Background="Transparent"
                                            Content="{StaticResource Agent}" 
                                            Height="260"/>
                                    <StackPanel Margin="0 30 0 0">
                                        <Button TextElement.Foreground="{DynamicResource MaterialDesignBody}" Command="{Binding FilmFrame}" FontSize="20" HorizontalAlignment="Left" FontWeight="Medium" BorderThickness="0" Background="Transparent" Content="Belet Video" Height="auto"/>
                                        <Button  Foreground="#377DFF" FontSize="17" HorizontalAlignment="Left" FontWeight="Medium"    BorderThickness="0" Background="Transparent" Content="Geç >" Command="{Binding FilmFrame}"  Style="{StaticResource MaterialDesignFlatButton}" materialDesign:ShadowAssist.ShadowDepth="Depth0"   Width="auto" materialDesign:ButtonAssist.CornerRadius="10"  />

                                    </StackPanel>
                                </StackPanel>
                            </Button.Content>
                            <Button.Triggers>
                                <EventTrigger RoutedEvent="Button.MouseEnter" >
                                    <BeginStoryboard>
                                        <Storyboard >
                                            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="Margin" BeginTime="00:00:00">
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.0"  Value="0, 0,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.1"  Value="0, -1,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.12" Value="0, -2,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.15" Value="0, -3,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.17" Value="0, -4,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.2"  Value="0, -5,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.23" Value="0, -6,0,20" />

                                            </ThicknessAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </EventTrigger>
                                <EventTrigger RoutedEvent="Button.MouseLeave" >
                                    <BeginStoryboard>
                                        <Storyboard >
                                            <ThicknessAnimationUsingKeyFrames Storyboard.TargetProperty="Margin" BeginTime="00:00:00">

                                                <SplineThicknessKeyFrame KeyTime="0:0:0.0" Value="0, -6,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.1" Value="0, -5,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.12" Value="0, -4,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.15"  Value="0, -3,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.17" Value="0, -2,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.2"  Value="0, -1,0,20" />
                                                <SplineThicknessKeyFrame KeyTime="0:0:0.23" Value="0, 0,0,20" />
                                            </ThicknessAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </BeginStoryboard>
                                </EventTrigger>
                            </Button.Triggers>
                        </Button>




                    </WrapPanel>








                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                        <Image Width="200" Margin="-500 0 0 0" Height="200">
                            <Image.Source>
                                <DrawingImage  PresentationOptions:Freeze="True">
                                    <DrawingImage.Drawing>
                                        <DrawingGroup>
                                            <DrawingGroup>
                                                <DrawingGroup>
                                                    <DrawingGroup.ClipGeometry>

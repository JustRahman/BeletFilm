            <Grid Margin="0 20 0 0">
                <StackPanel Width="520" HorizontalAlignment="Left" Margin="50 0 0 0" Orientation="Horizontal">

                    <TextBlock  Text="Поиск" FontSize="30" FontWeight="Medium"  />
                    <StackPanel  Margin="40 0 0 0">
                        <TextBlock Text="Категории" FontSize="12"/>
                        
                        <ComboBox  SelectedIndex="0" x:Name="MediatypeComboBox" HorizontalAlignment="Left"   Width="100" 
                                 FontSize="13"
                            
                            ItemsSource="{Binding Path=Kategory , Mode=TwoWay}"
                                     />
                    </StackPanel>
                    <StackPanel Margin="40 0 0 0">
                        <TextBlock Text="Жанр" FontSize="12"/>
                        <ComboBox SelectedIndex="0" HorizontalAlignment="Left"  x:Name="GenersComboBox"  Width="100" 
                               FontSize="13"
                            ItemsSource="{Binding Path=Geners , Mode=TwoWay}"
                                     />
                    </StackPanel>

                    <StackPanel Margin="40 0 0 0">
                        <TextBlock Text="Страны" FontSize="12"/>
                        <ComboBox HorizontalAlignment="Left"  x:Name="CountryComboBox" Width="100" 
                              FontSize="13"
                                  SelectedIndex="0"
                            ItemsSource="{Binding Path=Countries, Mode=TwoWay}"
                                     />
                    </StackPanel>



                </StackPanel>
                <StackPanel Margin="0 0 40 0" Orientation="Horizontal" HorizontalAlignment="Right">
                    <StackPanel>
                        <TextBlock Text="Название" FontSize="12"/>
                        <StackPanel Orientation="Horizontal">
                            <Border BorderBrush="Black"
                               BorderThickness="2"
                               VerticalAlignment="Center"
                               CornerRadius="5">
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon BorderBrush="Gray"  BorderThickness="0"   Height="25" Width="24" Kind="Magnify" />

                                    <TextBox Width="170" BorderBrush="Gray" x:Name="SearchText" BorderThickness="0" Height="25" />
                                </StackPanel>
                            </Border>



                        </StackPanel>
                    </StackPanel>
                    <StackPanel Margin="10 0 0 0">
                        <TextBlock Text="Сортировка" FontSize="12"/>
                        <ComboBox  x:Name="SortMediaCombobox" HorizontalAlignment="Left"   Width="150" 
                               FontSize="13"
                                    SelectedIndex="0"
                            ItemsSource="{Binding Path=SortMedia , Mode=TwoWay}"
                                     />
                    </StackPanel>
                </StackPanel>
            </Grid>
           

        </StackPanel>


        <ScrollViewer Margin="30 10 50 0" x:Name="BlockScrollViewer"  Grid.Row="2" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Hidden" >
            <WrapPanel Width="1000" x:Name="BlockWrapPanel">

            </WrapPanel>

        </ScrollViewer>


        <dxwui:NavigationFrame Grid.Row="1"   Grid.RowSpan="2" >
            <dxmvvm:Interaction.Behaviors>
                <dxmvvm:EventToCommand EventName="Loaded" Command="{Binding OnViewLoadedCommand}" />
                <dxwuin:FrameNavigationService />
            </dxmvvm:Interaction.Behaviors>

           
            
        </dxwui:NavigationFrame>

    </Grid>

    <dxmvvm:Interaction.Behaviors>
        <dxmvvm:EventToCommand EventName="Loaded"   Command="{Binding InitializeCommand}">
            <dxmvvm:EventToCommand.CommandParameter>
                <MultiBinding Converter="{StaticResource MultiBindingConverter}">
                    <Binding ElementName="Window"/>
                    <Binding ElementName="FirstGrid"/>
                    <Binding ElementName="BlockScrollViewer"/>
                    <Binding ElementName="BlockWrapPanel"/>
                    <Binding ElementName="MediatypeComboBox"/>
                    <Binding ElementName="CountryComboBox"/>
                    <Binding ElementName="GenersComboBox"/>
                    <Binding ElementName="SortMediaCombobox"/>
                    <Binding ElementName="LogOutBtn"/>
                    <Binding ElementName="SearchText"/>
                </MultiBinding>
            </dxmvvm:EventToCommand.CommandParameter>
        </dxmvvm:EventToCommand>


        <dxmvvm:EventToCommand EventName="SizeChanged"   Command="{Binding SizeChangedEvent}">
            <dxmvvm:EventToCommand.CommandParameter>
                <MultiBinding Converter="{StaticResource MultiBindingConverter}">
                    <Binding ElementName="Window"/> 
                    <Binding ElementName="BlockWrapPanel"/>
                </MultiBinding>
            </dxmvvm:EventToCommand.CommandParameter>
        </dxmvvm:EventToCommand>


    </dxmvvm:Interaction.Behaviors>



</Window>
